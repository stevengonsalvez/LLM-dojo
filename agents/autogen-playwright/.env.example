# LLM Configuration
# Available providers: openai, anthropic, azure
LLM_PROVIDER=openai

# API Keys (uncomment and set the one you need)
# LLM_API_KEY=sk-xxx                  # For OpenAI
# LLM_API_KEY=sk-ant-xxx             # For Anthropic
# LLM_API_KEY=azure-key-xxx          # For Azure

# Model Configuration
# OpenAI models: gpt-4, gpt-3.5-turbo, etc.
# Anthropic models: claude-2, claude-instant-1, etc.
# Azure: your-deployment-name
LLM_MODEL=gpt-4

# Optional Configuration
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=2000
LLM_REQUEST_TIMEOUT=600

# Playwright Configuration
SCREENSHOTS_DIR=screenshots
DEFAULT_TIMEOUT=30000
BROWSER_HEADLESS=false 